        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenCV)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.NHANVIENs.MaCV)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NHANVIENs.TenNV)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenCV)
    </td>
