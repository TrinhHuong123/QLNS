}

<h2>Index</h2>

<p>
    @Html.ActionLink("Thêm mới chức vụ", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NHANVIENs.MaCV)
        </th>
        <th>
